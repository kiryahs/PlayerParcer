{
    "manifest_version": 3,
    "name": "Парсер ставок Fonbet",
    "version": "1.5",
    "description": "Расширение для парсинга ID ставок на сайте Fonbet.",
    "permissions": [
      "storage",
      "scripting"
    ],
    "host_permissions": [
      "https://fon.bet/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://fon.bet/live*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  